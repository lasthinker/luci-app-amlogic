
config amlogic 'config'
	option enable '1'
	option amlogic_firmware_repo 'lasthinker/amlogic-s9xxx-openwrt'
	option amlogic_firmware_tag 's9xxx_lede'
	option amlogic_firmware_suffix '.img.gz'
	option amlogic_firmware_config '1'
	option amlogic_kernel_path 'https://github.com/lasthinker/amlogic-s9xxx-openwrt/tree/main/amlogic-s9xxx/amlogic-kernel'
	option amlogic_write_bootloader '1'

